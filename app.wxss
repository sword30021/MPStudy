/**app.wxss**/

.layout {
    display: flex;
    flex-direction: row;
    width: 100%;
    background-color: white;
    padding: 30rpx 15rpx;
}

.layoutLeft image {
    width: 50rpx;
    height: 50rpx;
}

.layoutRight image {
    width: 50rpx;
    height: 50rpx;
}

.layoutCenter {
    width: 70%;
}

.layoutLeft, .layoutRight {
    display: flex;
    flex-direction: row;
    justify-content: center;
    width: 15%;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
    background-color: whitesmoke;
}
